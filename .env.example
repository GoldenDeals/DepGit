# Git SSH server configuration
DEPGIT_SSH_GIT_ADDRESS=:2222
DEPGIT_SSH_GIT_HOSTKEY=./keys/ssh_host_key

# Storage configuration
DEPGIT_STORAGE_PATH=./storage

# Web server configuration
DEPGIT_WEB_ADDRESS=:8080
DEPGIT_WEB_STATIC_DIR=./web/dist

# MinIO configuration (if using MinIO storage)
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_ENDPOINT=localhost:9000
MINIO_BUCKET_NAME=depgit
MINIO_USE_SSL=false
